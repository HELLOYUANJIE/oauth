<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gosafe.oauth.dao.OauthAccessTokenDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.gosafe.oauth.entity.SysAccessTokenEntity" id="oauthAccessTokenMap">
        <result property="tokenId" column="token_id"/>
        <result property="token" column="token"/>
        <result property="authenticationId" column="authentication_id"/>
        <result property="userName" column="user_name"/>
        <result property="clientId" column="client_id"/>
        <result property="authentication" column="authentication"/>
        <result property="refreshToken" column="refresh_token"/>
    </resultMap>


</mapper>